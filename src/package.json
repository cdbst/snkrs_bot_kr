{
  "name": "snkrs_bot_kr",
  "version": "1.0.0",
  "description": "Nike SNKRS BOT for korean",
  "main": "app.js",
  "scripts": {
    "prebuild": "cd .. && npx babel ./src/view --out-dir ./src --presets react-app/prod",
    "build": "electron-packager . \"SNKRS Bot KR\" --platform win32 --arch x64 --out ../dist --prune",
    "postbuild": "npx asar pack \"../dist/SNKRS Bot KR-win32-x64/resources/app\" \"../dist/SNKRS Bot KR-win32-x64/resources/app.asar\" && rmdir /s /q \"../dist/SNKRS Bot KR-win32-x64/resources/app\"",
    "start": "electron ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdbst/snkrs_bot_kr.git"
  },
  "keywords": [
    "electrion",
    "js",
    "react"
  ],
  "author": "cdbst",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cdbst/snkrs_bot_kr/issues"
  },
  "homepage": "https://github.com/cdbst/snkrs_bot_kr#readme",
  "dependencies": {
    "async-mutex": "^0.3.2",
    "axios": "^0.24.0",
    "axios-proxy-fix": "^0.16.3",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "cheerio": "^1.0.0-rc.10",
    "cookie": "^0.4.1",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.12.28"
  },
  "devDependencies": {
    "asar": "^3.1.0",
    "electron": "^14.0.1",
    "electron-packager": "^15.4.0"
  }
}
